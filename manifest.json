{
  "manifest_version": 2,
  "name": "MyMixify",
  "version": "1.0",
  "description": "Convert Youtube's My Mix to Spotify Playlists",
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "MyMixify"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "icons": {
    "128": "icon.png"
  },
  "permissions": [
    "tabs",
    "http://127.0.0.1:5000/*",
    "https://www.youtube.com/*"
  ]
}